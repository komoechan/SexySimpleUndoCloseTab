{
  "manifest_version": 3,
  "name": "__MSG_extensionName__",
  "version": "1.4",
  "description": "__MSG_extensionDescription__",
  "default_locale": "en",
  "permissions": [
    "tabs",
    "history",
    "storage",
    "favicon"
  ],
  "host_permissions": ["*://*/*"],
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}